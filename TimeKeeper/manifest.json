{
	"name": "TimeKeeper",
	"description": "Keep track of your hours for clients and admin time.",
	"version": "2.2.3",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["js/background.js"],
			"persistant": false
		}
	},
	"permissions": [
		"storage"
	],
	"icons": { "16": "icons/tk-16.png", "128": "icons/tk-128.png" }
}